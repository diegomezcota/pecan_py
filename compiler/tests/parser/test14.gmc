program animales;

class Animal {
    var string nombre;
    var int edad;
    var string tipo;
};

constructor Animal(string nombre, int edad, string tipo) {
    $nombre = nombre;
    $edad = edad;
    $tipo = tipo;
}

obj a = Animal("a", 9, "a");

