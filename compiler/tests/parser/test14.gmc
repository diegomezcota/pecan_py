program animales;

class Animal {
    var string nombre;
    var int edad;
    var string tipo;
};

constructor Animal(string nombre, int edad, string tipo) {
    $nombre = nombre;
    $edad = edad;
    $tipo = tipo;
}


obj lambda = Animal("Lambda", 4, "gato");
obj nala = Animal("Nala", 5, "perro");

main() {
    write(lambda.nombre);
}