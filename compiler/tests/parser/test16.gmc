program a;


class Animal {
    var string nombre;
    var int edad;
    var string tipo;
};

constructor Animal(string nombre, int edad, string tipo) {
    $nombre = nombre;
    $edad = edad;
    $tipo = tipo;
}


obj shori = Animal("Tripsi", 3, "perringo");

main() {
    write(shori.nombre, shori.edad, shori.tipo);
}