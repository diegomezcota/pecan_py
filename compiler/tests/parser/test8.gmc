program a;

class Animal {
    var string nombre;
    var int edad;
    var string tipo;
};

constructor Animal(string nombre, int edad) {
    $nombre = nombre;
    $edad = edad;
    $tipo = "trips";
    write("tripsi");
}

@class Animal function constructor_fake(string nombre, int edad) returns bool {
    $nombre = nombre;
    $edad = edad;
    return 2 == $edad;
}

@class Animal function comer(int aucsilio) returns void {
    var string nombre_de_shori;
    nombre_de_shori = $nombre;
    write("yuuum", nombre_de_shori);
    return;
}

obj shori = Animal();

main() {
    obj brunis = Animal();
    write(shori.constructor_fake("Tripsi", 4));
