program a;

class Animal {
    var string nombre;
    var int edad;
    var string tipo;
};

constructor Animal(string nombre, int edad) {
    $nombre = nombre;
    $edad = edad;
    $tipo = "trips";
    write("tripsi");
}

@class Animal function comer(int aucsilio) returns void {
    var string nombre_de_shori;
    nombre_de_shori = $nombre;
    write("yuuum", nombre_de_shori);
    return;
}

obj shori = Animal();

main() {
    var int i;
    //obj shori = Animal();
    shori.nombre = "Tripsi";
    shori.comer(1);
}